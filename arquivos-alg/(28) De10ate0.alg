algoritmo "De10ate0"
// Função :
// Autor :
// Data : 18/02/2020
// Seção de Declarações 
var
   contador: Inteiro
inicio
      contador <- 10
      Enquanto (contador >= 0) faca
          EscrevaL (contador)
          contador <- contador - 1
      FimEnquanto
      EscrevaL ("FINISHHH!!")
fimalgoritmo
