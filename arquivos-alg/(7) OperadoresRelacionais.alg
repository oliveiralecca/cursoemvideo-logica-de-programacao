algoritmo "OperadoresRelacionais"
// Função :
// Autor :
// Data : 11/02/2020
// Seção de Declarações 
var
   A, B, C: Inteiro
inicio
      A <- 2
      B <- 3
      C <- 5
      Escreval (A>B)
      Escreval (A<>B)
      Escreval (A>2)
      Escreval (C=A+B)
      Escreval (C>=B^A)
      Escreva (1=C%2)
fimalgoritmo
